{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.37/schema.json",
  "app_specific_configuration_path": "$Env:USERPROFILE/applications.json",
  "window_hiding_behaviour": "Cloak",
  "cross_monitor_move_behaviour": "Insert",
  "default_workspace_padding": 3,
  "default_container_padding": 1,
  "border": true,
  "border_width": 5,
  "border_offset": -1,
  "float_rule_placement": "Center",
  "stackbar": {
    "mode": "OnStack"
  },
  "manage_rules": [
    {
      "kind": "Exe",
      "id": "ESI-Term-Dev.exe",
      "matching_strategy": "Equals"
    },
    {
      "kind": "Exe",
      "id": "neovide.exe",
      "matching_strategy": "Equals"
    },
    {
      "kind": "Exe",
      "id": "Meld.exe",
      "matching_strategy": "Equals"
    },
    {
      "kind": "Exe",
      "id": "MicrosoftWhiteboard.exe",
      "matching_strategy": "Equals"
    },
    {
      "kind": "Exe",
      "id": "Taskmgr.exe",
      "matching_strategy": "Equals"
    },
    {
      "kind": "Exe",
      "id": "TouchGFXDesigner",
      "matching_strategy": "StartsWith"
    },
    {
      "kind": "Exe",
      "id": "IarIdepm.exe",
      "matching_strategy": "Equals"
    },
    {
      "kind": "Exe",
      "id": "CamBam.exe",
      "matching_strategy": "Equals"
    }
  ],
  "floating_applications": [
    {
      "kind": "Exe",
      "id": "ESI-Term-Dev.exe",
      "matching_strategy": "Equals"
    },
    {
      "kind": "Exe",
      "id": "CalculatorApp.exe",
      "matching_strategy": "Equals"
    },
    {
      "kind": "Exe",
      "id": "Taskmgr.exe",
      "matching_strategy": "Equals"
    }
  ],
  "ignore_rules": [
    [
      {
        "kind": "Exe",
        "id": "ESI-Term-Dev.exe",
        "matching_strategy": "Equals"
      },
      {
        "kind": "Title",
        "id": "ESI AutoMation",
        "matching_strategy": "DoesNotContain"
      }
    ]
  ],
  "theme": {
    "palette": "Base16",
    "name": "Github"
  },
  "monitors": [
    {
      "window_based_work_area_offset": {
        "top": 0,
        "left": 600,
        "right": 1200,
        "bottom": 0
      },
      "workspaces": [
        {
          "name": "Browser",
          "layout": "BSP",
          "apply_window_based_work_area_offset": true,
          "initial_workspace_rules": [
            {
              "kind": "Exe",
              "id": "thorium.exe",
              "matching_strategy": "Equals"
            }
          ]
        },
        {
          "name": "Code",
          "layout": "BSP",
          "apply_window_based_work_area_offset": true,
          "initial_workspace_rules": [
            {
              "kind": "Exe",
              "id": "devenv.exe",
              "matching_strategy": "Equals"
            }
          ]
        },
        {
          "name": "Windows Terminal",
          "layout": "BSP",
          "apply_window_based_work_area_offset": true,
          "initial_workspace_rules": [
            {
              "kind": "Exe",
              "id": "WindowsTerminal.exe",
              "matching_strategy": "Equals"
            },
            {
              "kind": "Exe",
              "id": "alacritty.exe",
              "matching_strategy": "Equals"
            }
          ]
        },
        {
          "name": "AutoCAD",
          "layout": "BSP",
          "apply_window_based_work_area_offset": true,
          "initial_workspace_rules": []
        },
        {
          "name": "I",
          "layout": "BSP",
          "apply_window_based_work_area_offset": true
        },
        {
          "name": "II",
          "layout": "BSP",
          "apply_window_based_work_area_offset": true
        },
        {
          "name": "III",
          "layout": "BSP",
          "apply_window_based_work_area_offset": true
        },
        {
          "name": "IV",
          "layout": "BSP",
          "apply_window_based_work_area_offset": true
        },
        {
          "name": "V",
          "layout": "BSP",
          "apply_window_based_work_area_offset": true
        },
        {
          "name": "VI",
          "layout": "BSP",
          "apply_window_based_work_area_offset": true
        },
        {
          "name": "VII",
          "layout": "BSP",
          "apply_window_based_work_area_offset": true
        },
        {
          "name": "VIII",
          "layout": "BSP",
          "apply_window_based_work_area_offset": true
        },
        {
          "name": "IX",
          "layout": "BSP",
          "apply_window_based_work_area_offset": true
        },
        {
          "name": "Git",
          "layout": "BSP",
          "apply_window_based_work_area_offset": true,
          "initial_workspace_rules": [
            {
              "kind": "Exe",
              "id": "GitKraken"
            },
            {
              "kind": "Exe",
              "id": "GitButler"
            },
            {
              "kind": "Exe",
              "id": "sublime_merge.exe"
            }
          ]
        },
        {
          "name": "Social",
          "layout": "BSP",
          "apply_window_based_work_area_offset": true,
          "initial_workspace_rules": [
            {
              "kind": "Exe",
              "id": "ms-teams.exe",
              "matching_strategy": "Equals"
            },
            {
              "kind": "Exe",
              "id": "olk.exe",
              "matching_strategy": "Equals"
            }
          ]
        }
      ]
    }
  ]
}
